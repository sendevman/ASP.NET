  <connectionStrings>
    <add name="Taskever" connectionString="Server=localhost; Database=Taskever; Trusted_Connection=True;" />
  </connectionStrings>

  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="recaptchaPublicKey" value="6LcfHusSAAAAAD2B88Gx_PcHGBVEWHhfMjs3yjKs" />
    <add key="recaptchaPrivateKey" value="6LcfHusSAAAAAGvtJmw3BajnO2q5bqQk6Q7bi9-T" />
    <add key="Email.SenderAddress" value="mailer@taskever.com" />
    <add key="Email.SenderPassword" value="" />
    <add key="Email.Server" value="" />
  </appSettings>
  
  <system.web>
    <authentication mode="Forms">
      <forms loginUrl="~/Account/Login" timeout="86400" />
    </authentication>
    <globalization enableClientBasedCulture="true" culture="auto" uiCulture="auto" />
    <membership defaultProvider="SestekMembershipProvider">
      <providers>
        <clear />
        <add name="SestekMembershipProvider" type="Abp.Modules.Core.Mvc.Web.Authentication.AbpMembershipProvider" />
      </providers>
    </membership>
    <sessionState timeout="7200" />
    <customErrors mode="RemoteOnly" />
    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime targetFramework="4.5.1" />
  </system.web>

  <system.web.webPages.razor>
    <host factoryType="System.Web.Mvc.MvcWebRazorHostFactory, System.Web.Mvc, Version=5.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
    <pages pageBaseType="Taskever.Web.Mvc.Views.TaskeverViewBase">
      <namespaces>
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Optimization" />
        <add namespace="System.Web.Routing" />
        <add namespace="Taskever.Web.Mvc" />
        <add namespace="Abp.Web.Mvc.Helpers" />
      </namespaces>
    </pages>
  </system.web.webPages.razor>

  <system.webServer>
    <modules>
      <remove name="FormsAuthenticationModule" />
    </modules>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  
